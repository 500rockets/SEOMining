# SEO Mining Configuration
# Copy this file to .env and fill in your actual values

# ValueSerp API Configuration
VALUESERP_API_KEY=your_api_key_here
VALUESERP_BASE_URL=https://api.valueserp.com/search

# Search Configuration
DEFAULT_LOCATION=98146, Washington, United States
DEFAULT_GL=us
DEFAULT_HL=en
DEFAULT_GOOGLE_DOMAIN=google.com

# Compute Configuration
# Options: light, medium, heavy
COMPUTE_PROFILE=medium

# GPU Settings
USE_GPU=true
GPU_DEVICE=0

# Storage Configuration
DATA_DIR=./data
CACHE_ENABLED=true
CACHE_TTL_DAYS=7

# Scraping Configuration
USER_AGENT=SEOMiningBot/1.0 (Educational/Research)
REQUEST_DELAY_SECONDS=0.5
MAX_CONCURRENT_REQUESTS=10
RESPECT_ROBOTS_TXT=true

# Proxy Configuration
USE_PROXIES=true
PROXY_FILE=config/proxies.txt
PROXY_ROTATION=round-robin
PROXY_TIMEOUT_SECONDS=10
PROXY_RETRY_ATTEMPTS=3
PROXY_HEALTH_CHECK_INTERVAL=300
DISABLE_DIRECT_CONNECTION=true

# Docker Configuration (for container runtime)
DOCKER_GPU_ENABLED=true
NVIDIA_VISIBLE_DEVICES=all

